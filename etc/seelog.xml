<seelog>
<outputs formatid="main">
    <filter levels="info,debug,critical,error">
        <console />
    </filter>
    <filter levels="debug">
        <file path="debug.log" />
    </filter>
</outputs>
<formats>
    <format id="main" format="%Date %Time [%LEV] [%File:%Line] [%Func] %Msg%n" />
</formats>
</seelog>